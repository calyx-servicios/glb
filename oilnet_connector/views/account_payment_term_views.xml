<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_payment_term_form_inherit_oilnet" model="ir.ui.view">
		<field name="name">account.payment.term.oilnet</field>
		<field name="model">account.payment.term</field>
		<field name="inherit_id" ref="account.view_payment_term_form"/>
		<field name="arch" type="xml">
			<xpath expr="//group" position="after">
				<group name="oilnet_information" string="Oilnet Information">
					<field name="oilnet_gesal_code"/>
					<field name="oilnet_barranca_code" />
				</group>
			</xpath>
		</field>
	</record>
</odoo>